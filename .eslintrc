{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021
  },

  "env": {
      "es6": true,
      "commonjs": true,
      "node": true,
      "mocha": true
  },
  "extends": [
      "eslint:recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-multiple-empty-lines": "error",
    "space-before-function-paren": "error"
  },
  "ignorePatterns": ["./**/*.test.ts", "./node_modules/**/*"]
}